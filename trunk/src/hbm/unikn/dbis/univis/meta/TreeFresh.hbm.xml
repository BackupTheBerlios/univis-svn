<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="unikn.dbis.univis.meta.TreeFresh" table="META_TREE_FRESH">

        <id name="id" type="long" unsaved-value="-1">
            <column name="ID" not-null="true"/>
            <generator class="native"/>
        </id>

        <many-to-one name="dataReference" column="DATA_REFERENCE_ID" class="unikn.dbis.univis.meta.impl.DataReferenceImpl"/>

        <list name="children" table="META_TREE_FRESH_RELATION">
            <key column="PARENT_ID" not-null="true"/>
            <index column="CHILD_ORDER"/>
            <many-to-many column="CHILD_ID" class="unikn.dbis.univis.meta.TreeFresh"/>
        </list>

    </class>

</hibernate-mapping>