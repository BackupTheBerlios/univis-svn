<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <joined-subclass name="unikn.dbis.univis.meta.impl.DimensionImpl" table="META_DIMENSION" extends="unikn.dbis.univis.meta.TreeFresh">

        <key column="TREE_FRESH_ID"/>

        <property name="summable" type="boolean">
            <column name="IS_ABSTRACT" not-null="true"/>
        </property>

        <set name="supportedCubes" table="META_DIMENSION_SUPPORTED_CUBE">
            <key column="DIMENSION_ID" not-null="true"/>
            <many-to-many column="CUBE_ID" class="unikn.dbis.univis.meta.impl.CubeImpl"/>
        </set>

        <property name="level" type="integer">
            <column name="LEVEL" not-null="false"/>
        </property>

        <property name="category" type="string">
            <column name="CATEGORY" not-null="false"/>
        </property>

    </joined-subclass>

</hibernate-mapping>